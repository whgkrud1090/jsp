<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
  <select id="getUserList" resultType="userVo">
<!--   resultType="kr.or.ddit.user.model.UserVo" : Vo의 경로를 입력해준다. -->
    select * 
      from users
  </select>
  
  <select id="getUser" parameterType="String" resultType="userVo">
  	select * 
  	  from users
  	 where userId = #{userId}
  </select>
  
  <select id="getUserListHalf" parameterType="String" resultType="userVo">
   select * from users
      where rownum &lt; = 50
  </select>
</mapper>